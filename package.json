{
  "name": "express-typeorm-rest-boilerplate",
  "version": "0.1.0",
  "description": "Boilerplate code to get started with building RESTful API Services",
  "main": "build/app.js",
  "scripts": {
    "start": "node build/app.js",
    "build": "tsc -p .",
    "dev": "nodemon -V --exec ts-node src/app.ts",
    "mongodb": "mkdirp .mongodb && mongod --dbpath .mongodb",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test:unix": "NODE_ENV=test jest",
    "test": "set NODE_ENV=test&jest",
    "seed": "ts-node -T commands/seed.ts",
    "generate": "ts-node -T commands/generate.ts",
    "semantic-release": "semantic-release"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bymi15/express-typeorm-rest-boilerplate.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.com/"
  },
  "keywords": [
    "express-typeorm-rest-boilerplate",
    "javascript",
    "typescript",
    "nodejs",
    "express",
    "typeorm",
    "typedi",
    "boilerplate",
    "api",
    "rest",
    "jest",
    "validation",
    "mongodb"
  ],
  "author": "Brian Min <bymi15@yahoo.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bymi15/express-typeorm-rest-boilerplate/issues"
  },
  "homepage": "https://github.com/bymi15/express-typeorm-rest-boilerplate#readme",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "celebrate": "^13.0.3",
    "class-validator": "^0.12.2",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "esm": "3.2.25",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "helmet": "^4.1.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.6.2",
    "reflect-metadata": "^0.1.13",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.26",
    "typeorm-typedi-extensions": "^0.2.3",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/bcrypt": "^3.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.8",
    "@types/express-jwt": "0.0.42",
    "@types/faker": "^5.1.2",
    "@types/jest": "^26.0.14",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.11.2",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "commander": "^6.1.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "faker": "^5.1.0",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "mkdirp": "^1.0.4",
    "nodemon": "^2.0.4",
    "pluralize": "^8.0.0",
    "prettier": "^2.1.2",
    "semantic-release": "^17.1.2",
    "ts-jest": "^26.4.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  }
}
